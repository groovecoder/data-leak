{
  "manifest_version": 2,
  "name": "Data Leak",
  "version": "1.0",

  "description": "Logs potentially unsafe data leaving the browser. Inspired by CLIQZ and OpenWPM.",

  "icons": {
    "48": "icon.png",
    "96": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
  "scripts": ["webRequest.bundle.js."]
}
}
